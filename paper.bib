	month = mar,
	year = {2015},
	note = {Number: 1},
	keywords = {Intergroup Dynamics (major), Prejudice, Child Attitudes (major), Childhood Development (major), Cognitive Development (major), Psychosocial Development (major)},
	pages = {50--54},
}


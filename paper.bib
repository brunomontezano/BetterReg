	author = {Son Hing, Leanne S. and Bobocel, D. Ramona and Zanna, Mark P. and Garcia, Donna M. and Gee, Stephanie S. and Orazietti, Katie},
	month = sep,
	year = {2011},
	keywords = {Prejudice, Diversity, Female, Humans, Male, Adult, Adolescent, Motivation, Social Justice, Young Adult, affirmative action, Affirmative Action, Ideology, Middle Aged, Politics, Authoritarianism, Awards (Merit), diversity initiatives, Dominance Hierarchy, justice, Justice, meritocracy, Social Dominance, Society, system justification},
	pages = {433--450},
	file = {EBSCO Full Text:C\:\\Users\\cla18\\Zotero\\storage\\T9RUCRR5\\Son Hing et al. - 2011 - The merit of meritocracy.pdf:application/pdf},
}


	author = {Dunham, Yarrow and Chen, Eva E. and Banaji, Mahzarin R.},
	month = jun,
	year = {2013},
	note = {Number: 6},
	keywords = {Ingroup Outgroup (major), Developmental Psychology (major), Implicit Memory (major), Internalization (major), Social Sciences (major)},
	pages = {860--868},
}

@article{dunham_children_2007,

}

@article{son_hing_meritocracy_2002,
	title = {Meritocracy and opposition to affirmative action: {Making} concessions in the face of discrimination},
	volume = {83},
	issn = {0022-3514},
	shorttitle = {Meritocracy and opposition to affirmative action},
	url = {http://ezproxy.humboldt.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2002-17813-001&site=ehost-live},
	doi = {10.1037/0022-3514.83.3.493},
	abstract = {Typically, people who strongly endorse the merit principle and believe that outcomes should be given to those most deserving oppose affirmative action (AA) programs that violate this principle. However, how do they respond to AA when faced with a great deal of workplace discrimination? The authors hypothesized that people who care strongly about merit should be motivated to combat discrimination because it biases the assessment of merit. Consequently, these individuals should make concessions for AA. The authors found support for their hypothesis when investigating (a) participants' preexisting perceptions of workplace discrimination and (b) experimentally induced perceptions of discrimination. They discuss the implications of these results for the psychology of meritocracy and for resistance to AA. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},

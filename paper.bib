	keywords = {Ingroup Outgroup, Intergroup Dynamics, Threat, intergroup relations, Emotional Responses, cognitive and affective responses, groups, ingroup, intergroup threat theory, outgroup, realistic and symbolic threat, Theories},
	pages = {43--59},
}


	month = nov,
	year = {2011},
	note = {Number: 4},
	keywords = {Friendship (major), Intergroup Dynamics (major), Prejudice, Ingroup Outgroup (major), Stereotyped Attitudes},
	pages = {332--351},
}

@article{dunham_two_2013-1,
	title = {Two signatures of implicit intergroup attitudes: {Developmental} invariance and early enculturation},
	volume = {24},
	copyright = {© 2013, The Author(s)},
	issn = {0956-7976},
	shorttitle = {Two signatures of implicit intergroup attitudes},
	url = {http://search.proquest.com/psycinfo/docview/1370839556/37CEF42077574079PQ/3},
	doi = {http://dx.doi.org/10.1177/0956797612463081},
	abstract = {Long traditions in the social sciences have emphasized the gradual internalization of intergroup attitudes and the putatively more basic tendency to prefer the groups to which one belongs. In four experiments ( N = 883) spanning two cultures and two status groups within one of those cultures, we obtained new evidence that implicit intergroup attitudes emerge in young children in a form indistinguishable from adult attitudes. Strikingly, this invariance from childhood to adulthood holds for members of socially dominant majorities, who consistently favor their in-group, as well as for members of a disadvantaged minority, who, from the early moments of race-based categorization, do not show a preference for their in-group. Far from requiring a protracted period of internalization, implicit intergroup attitudes are characterized by early enculturation and developmental invariance. (PsycINFO Database Record (c) 2016 APA, all rights reserved) (Source: journal abstract)},

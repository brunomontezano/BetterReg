@article{sana_improving_2021,
	title = {Improving conceptual learning via pretests},
	volume = {27},
	issn = {1076-898X},
	url = {http://ezproxy.humboldt.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2020-79125-001&site=ehost-live},
	doi = {10.1037/xap0000322},
	abstract = {Although examples can be structured to emphasize diagnostic features of concepts, novice learners tend to focus on irrelevant surface features and struggle to encode deeper structures. Experiment 1 examined whether pretesting—answering questions about content before it is studied—could enhance learners’ noticing of diagnostic features, making them easier to process during subsequent study. Participants studied statistical concepts with examples that emphasized surface details or deep structure, and then classified new examples of these concepts. Studying examples that emphasized deep structure increased classification performance compared to examples that emphasized surface details. Moreover, taking pretests prior to studying the examples increased classification performance and eliminated differential benefits of studying structure versus surface examples. Experiment 2 examined whether pretesting serves a role beyond directing attention. After studying different statistical concepts with only surface-emphasizing examples, classification performance was better when participants actually took pretests compared to being given the correct responses. It is the generative aspect of pretesting, beyond attention directing, that improves conceptual learning among novice learners. (PsycInfo Database Record (c) 2021 APA, all rights reserved)},
	number = {2},
	urldate = {2021-08-23},
	journal = {Journal of Experimental Psychology: Applied},
	author = {Sana, Faria and Yan, Veronica X. and Clark, Courtney M. and Bjork, Elizabeth Ligon and Bjork, Robert A.},
	month = jun,
	year = {2021},
	note = {Publisher: American Psychological Association},
	keywords = {Concepts, Deep Neural Networks, deep structure, example-based learning, Learning, pretesting, Pretesting, Psychodiagnostic Typologies, Responses},
	pages = {228--236},
	file = {EBSCO Full Text:C\:\\Users\\cla18\\Zotero\\storage\\UMHQ3B4Y\\Sana et al. - 2021 - Improving conceptual learning via pretests.pdf:application/pdf},
}

@article{kornell_why_2011,
	title = {Why tests appear to prevent forgetting: {A} distribution-based bifurcation model},
	volume = {65},
	issn = {0749-596X},
	shorttitle = {Why tests appear to prevent forgetting},
	url = {https://www.sciencedirect.com/science/article/pii/S0749596X11000416},
	doi = {10.1016/j.jml.2011.04.002},
	abstract = {Retrieving information from memory produces more learning than does being presented with the same information, and the benefits of such retrieval appear to grow as the delay before a final recall test grows longer. Recall tests, however, measure the number of items that are above a recall threshold, not memory strength per se. According to the model proposed in this paper, tests without feedback produce bifurcated item distributions: Retrieved items become stronger, but non-retrieved items remain weak, resulting in a gap between the two classes of items. Restudying items, on the other hand, strengthens all items, though to a lesser degree than does retrieval. These differing outcomes can make tested items appear to be forgotten more slowly than are restudied items—even if all items are forgotten at the same rate—because the test-induced bifurcation leaves items either well above or well below threshold. We review prior evidence and present three new experiments designed to test the bifurcation interpretation.},
	language = {en},
	number = {2},
	urldate = {2021-08-23},
	journal = {Journal of Memory and Language},
	author = {Kornell, Nate and Bjork, Robert A. and Garcia, Michael A.},
	month = aug,
	year = {2011},
	keywords = {Learning, Memory, Retention interval, Retrieval, Testing},
	pages = {85--97},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\cla18\\Zotero\\storage\\BS5F9IW9\\Kornell et al. - 2011 - Why tests appear to prevent forgetting A distribu.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\cla18\\Zotero\\storage\\VBEYZ8KX\\S0749596X11000416.html:text/html},
}

@article{kornell_stability_2009,
	title = {A stability bias in human memory: {Overestimating} remembering and underestimating learning.},
	volume = {138},
	issn = {1939-2222, 0096-3445},
	shorttitle = {A stability bias in human memory},
	url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0017350},
	doi = {10.1037/a0017350},
	abstract = {The dynamics of human memory are complex and often unintuitive, but certain features—such as the fact that studying results in learning—seem like common knowledge. In 12 experiments, however, participants who were told they would be allowed to study a list of word pairs between 1 and 4 times and then take a cued-recall test predicted little or no learning across trials, notwithstanding their large increases in actual learning. When queried directly, the participants espoused the belief that studying results in learning, but they showed little evidence of that belief in the actual task. These findings, when combined with A. Koriat, R. A. Bjork, L. Sheffer, and S. K. Bar’s (2004) research on judgments of forgetting, suggest a stability bias in human memory—that is, a tendency to assume that the accessibility of one’s memories will remain relatively stable over time rather than benefiting from future learning or suffering from future forgetting.},
	language = {en},
	number = {4},
	urldate = {2021-08-23},
	journal = {Journal of Experimental Psychology: General},
	author = {Kornell, Nate and Bjork, Robert A.},

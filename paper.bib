	volume = {48},
	copyright = {© 2009, The British Psychological Society},
	issn = {0144-6665},
	url = {http://search.proquest.com/psycinfo/docview/621990378/106B89C4C59F4935PQ/1},
	doi = {http://dx.doi.org/10.1348/014466608X387089},
	abstract = {In a sample of White Australians (N = 273), cross-group friendship with Aboriginal Australians was associated with reduced cognitions of rejection and intergroup anxiety, and these variables fully mediated the effect of cross-group friendship on conversational avoidance of sensitive intergroup topics, active avoidance of the outgroup, and old-fashioned prejudice. The novel mediator proposed here, cognitions of rejection, predicted intergroup anxiety, and also predicted the three outcome variables via intergroup anxiety. Over and above its indirect effects via anxiety, cognitions of rejection directly predicted both conversational and active avoidance, suggesting that whilst the cognitive and affective mediators are linked, they predict intergroup outcomes in different ways. The results demonstrate the beneficial impact of cross group friendship in reducing prejudice and avoidance by diminishing cognitions of rejection and intergroup anxiety. We also highlight that individuals without cross-group friends may perceive the outgroup as rejecting, feel anxious about cross-group interaction, and desire both conversational and physical avoidance of the outgroup. (PsycINFO Database Record (c) 2016 APA, all rights reserved) (Source: journal abstract)},
	language = {English},
	number = {3},
	urldate = {2018-08-12},
	journal = {British Journal of Social Psychology},
	author = {Barlow, Fiona Kate and Louis, Winnifred R. and Hewstone, Miles},
	month = sep,
	year = {2009},
	note = {Number: 3},
	keywords = {Anxiety (major), Friendship (major), Avoidance, Cognitions (major), Group Dynamics (major), Indigenous Populations (major), Ingroup Outgroup, Interpersonal Relationships, Prejudice},
	pages = {389--405},
}

@article{baron_constraints_2015,

@article{henry_contact_2006-4,
	title = {The {Contact} {Hypothesis} {Revisited}: {Status} {Bias} in the {Reduction} of {Implicit} {Prejudice} in the {United} {States} and {Lebanon}},
	volume = {17},
	issn = {0956-7976},

	keywords = {Anxiety, Friendship (major), Intergroup Dynamics (major), Prejudice (major), Racial and Ethnic Relations, Ingroup Outgroup (major), Whites, Self-Disclosure (major), Elementary School Students, High School Students, South Asian Cultural Groups},
	pages = {369--388},
}


	volume = {n/a},
	copyright = {© 2019 The Society for the Psychological Study of Social Issues},
	issn = {1530-2415},
	url = {https://spssi.onlinelibrary.wiley.com/doi/abs/10.1111/asap.12176},
	doi = {10.1111/asap.12176},
	abstract = {Sexual assault is a pervasive problem on college campuses. Federal laws have shaped policies addressing sexual assault in higher education. Title IX guidance requires universities to designate “Responsible Employees,” who are required to report all sexual assaults they learn about to the university (even if the survivor does not want to report). Resident assistants (RAs) are an important source of support for students and increasingly designated as Responsible Employees. University mandatory reporting policies have significant implications for reporters’ and survivors’ lives, but there is a lack of research on how Responsible Employees perceive this role. The current study examined Responsible Employee RA perceptions of mandatory reporting, the relationship between RAs’ experience of college sexual assault and perceptions of mandatory reporting, and whether trust in university response to sexual assault mediates this relationship. Data from 305 Responsible Employee RAs were analyzed. RAs’ perceptions of their reporting role were varied. For instance, RAs believed that mandatory reporting complicates their other responsibilities. College sexual assault survivors had less trust in university response to sexual assault, which, in turn, predicted more negative perceptions of mandatory reporting. Responsible Employees’ perceptions of mandatory reporting offer valuable insight into the potential effectiveness and drawbacks of these policies.},
	language = {en},
	number = {n/a},
	urldate = {2019-11-26},
	journal = {Analyses of Social Issues and Public Policy},
	author = {Holland, Kathryn J.},
	file = {Snapshot:C\:\\Users\\cla18\\Zotero\\storage\\SBB3WEME\\asap.html:text/html},
}


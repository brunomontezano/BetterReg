	journal = {Psychometrika},
	author = {Zhang, Zhiyong and Wang, Lijuan},
	month = jan,
	year = {2013},
	keywords = {auxiliary variables, bootstrap, mediation analysis, MI, missing data, TS-ML},
	pages = {154--184},
}


	journal = {Journal of Personality and Social Psychology},
	author = {Vonofakou, Christiana and Hewstone, Miles and Voci, Alberto},
	month = may,
	year = {2007},
	keywords = {Friendship (major), Intergroup Dynamics (major), Prejudice, Adult Attitudes (major), Ingroup Outgroup (major), Male Homosexuality (major)},
	pages = {804--820},
}


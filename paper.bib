	keywords = {Friendship (major), Prejudice (major), Male Homosexuality (major), Whites, Homosexuality (Attitudes Toward) (major), Lesbianism (major), Heterosexuality, Human Females},
	pages = {447--455},
	file = {Lytle and Levy - 2015 - Reducing heterosexuals’ prejudice toward gay men a.pdf:C\:\\Users\\cla18\\Zotero\\storage\\MIZMUZKC\\Lytle and Levy - 2015 - Reducing heterosexuals’ prejudice toward gay men a.pdf:application/pdf},

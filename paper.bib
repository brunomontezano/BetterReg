	note = {Publisher: SAGE Publications Inc},
	keywords = {disclosure, methodology, motivated reasoning, publication},
	pages = {1359--1366},
	file = {SAGE PDF Full Text:C\:\\Users\\cla18\\Zotero\\storage\\BR9IF52G\\Simmons et al. - 2011 - False-Positive Psychology Undisclosed Flexibility.pdf:application/pdf},
}

